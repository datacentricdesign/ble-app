{
    "name": "BLE App",
    "version": "0.0.1",
    "environment": "production",
    "basehref": "ble-app",
    "apiURL": "https://dwd.tudelft.nl/bucket/api",
    "authCodeFlow": {
        "issuer": "https://dwd.tudelft.nl:443/",
        "redirectUri": "https://datacentricdesign.github.io/ble-app",
        "postLogoutRedirectUri": "https://datacentricdesign.github.io/ble-app",
        "clientId": "clients:ble-app",
        "responseType": "code",
        "scope": "openid offline profile email dcd:public dcd:things dcd:properties",
        "useSilentRefresh": false,
        "showDebugInformation": true,
        "sessionChecksEnabled": false,
        "timeoutFactor": 0.01,
        "clearHashAfterLogin": false
      }
}